{
  "reporter": "raw",
  "watch": false,
  "path": "./styles",
  "excludes": ["node_modules/"],
  "rules": {
    "depthControl": {
      "indentPref": 2
    },
    "mixedSpaces": {
      "indentPref": 2
    },
    "quotePref": ["single"],
    "useMixinInsteadUnit": false,
    "color": {
      "denyRGB": false
    }
  }
}
